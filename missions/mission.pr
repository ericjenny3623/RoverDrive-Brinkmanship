# To run a mission, do this:
#  roslaunch launch/server.launch bag:=true name:=mission_name
#  rosrun goto goto_node.py _mission:='~/RoverDrive/missions/<mission-file>.pr'

# Configure the PitCam
IMG_PATH "~/Images/mission_day_1_number_1/"
EXP 1600, 2400, 3200, 6400, 9300
ABS
PAN -60, -45, -30, -15, 0, 15, 30, 45, 60
TILT 0, 10, 20, 30, 40, 50, 60, 70

# Drive to pit rim.
ABS 10.0, -10.0, 280, BRINK_OFF
REL 10.0,   0.0, 0.0, BRINK_ON

# Capture 1
CAP

# Back up
REL -10.0,  0.0, 0.0, BRINK_OFF
